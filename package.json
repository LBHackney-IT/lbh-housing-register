{
  "name": "hackney-housing-register",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next dev --hostname=localdev.hackney.gov.uk",
    "start": "next start",
    "lint": "eslint . --max-warnings 0 --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --fix --max-warnings 0 --ext .js,.jsx,.ts,.tsx",
    "prettier:test": "npx prettier -c .",
    "prettier:fix": "npx prettier --write .",
    "test:ci": "jest --ci --coverage --reporters=default --reporters=jest-junit && tsc --noEmit",
    "test": "jest && tsc --noEmit",
    "test:watch": "jest --watch",
    "prepare": "husky || true",
    "cypress:open": "cypress open --browser chrome",
    "e2e:run": "cypress run --e2e --browser electron",
    "component:run": "cypress run --component --browser electron"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix --max-warnings 0 --no-ignore",
      "prettier --ignore-unknown --write"
    ],
    "!(*.js|*.jsx|*.ts|*.tsx|package-lock.json)": "prettier --ignore-unknown --write"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.7",
    "@sentry/nextjs": "^7.119.2",
    "axios": "^0.21.1",
    "cookie": "^0.4.2",
    "csv-writer": "^1.6.0",
    "formik": "^2.2.9",
    "http-status-codes": "^2.3.0",
    "jsonwebtoken": "^9.0.2",
    "lbh-frontend": "^3.6.1",
    "lodash.throttle": "^4.1.1",
    "nested-object-diff": "^1.1.0",
    "next": "12.3.4",
    "next-redux-wrapper": "^6.0.2",
    "notifications-node-client": "^8.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "8.1.2",
    "redux": "4.2.1",
    "restana": "^4.9.9",
    "serve-static": "^1.15.0",
    "serverless-http": "^2.7.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.24.7",
    "@babel/preset-react": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@faker-js/faker": "^8.4.1",
    "@hackney/prettier-config": "^1.0.1",
    "@next/eslint-plugin-next": "12.3.4",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^12.1.5",
    "@types/cookie": "^0.4.1",
    "@types/jest": "29.5.4",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/lodash": "4.17.5",
    "@types/lodash.throttle": "^4.1.9",
    "@types/node": "22.5.5",
    "@types/react": "^17.0.80",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "babel-jest": "^29.7.0",
    "concurrently": "^6.5.1",
    "cypress": "^13.15.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-cypress": "^3.5.0",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-jest": "^28.8.3",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-testing-library": "4.9.0",
    "husky": "^9.1.6",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-junit": "^16.0.0",
    "lint-staged": "^15.2.10",
    "mockdate": "^3.0.5",
    "nock": "^14.0.0-beta.11",
    "node-mocks-http": "^1.15.1",
    "playwright-webkit": "^1.47.0",
    "prettier": "2.2.1",
    "sass": "^1.32.12",
    "ts-node": "^10.9.2",
    "typescript": "4.9.5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
