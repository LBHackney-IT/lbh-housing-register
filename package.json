{
  "name": "hackney-housing-register",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next dev --hostname=localdev.hackney.gov.uk",
    "start": "next start -p $PORT",
    "mocks": "mocks-server",
    "lint": "eslint . --max-warnings 0 --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --fix --max-warnings 0 --ext .js,.jsx,.ts,.tsx",
    "prettier:test": "npx prettier -c .",
    "prettier:fix": "npx prettier --write .",
    "test:ci": "tsc --noEmit && jest --ci",
    "test": "jest && tsc --noEmit",
    "test:watch": "jest --watch",
    "prepare": "husky",
    "cypress:open": "cypress open --browser chrome",
    "cypress:run": "cypress run --browser chrome"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --fix --max-warnings 0",
    "*": "prettier --ignore-unknown --write"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.5.1",
    "@sentry/nextjs": "^6.19.2",
    "axios": "^0.21.1",
    "cookie": "^0.4.1",
    "csv-writer": "^1.6.0",
    "dotenv": "^16.4.5",
    "formik": "^2.2.9",
    "http-status-codes": "^2.1.4",
    "jsonwebtoken": "^8.5.1",
    "lbh-frontend": "^3.5.2",
    "lodash.throttle": "^4.1.1",
    "nested-object-diff": "^1.1.0",
    "next": "^12.2.3",
    "next-redux-wrapper": "^6.0.2",
    "notifications-node-client": "^5.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^8.0.2",
    "redux": "^4.1.0",
    "restana": "^4.9.1",
    "serve-static": "^1.14.1",
    "serverless-http": "^2.7.0",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@hackney/eslint-config": "^1.4.0",
    "@hackney/prettier-config": "^1.0.1",
    "@mocks-server/main": "^2.3.1",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^12.1.5",
    "@types/cookie": "^0.4.0",
    "@types/jest": "^28.1.1",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/lodash.throttle": "^4.1.6",
    "@types/react": "^17.0.5",
    "@types/uuid": "^8.3.1",
    "@types/yup": "^0.29.11",
    "@typescript-eslint/eslint-plugin": "5.4.0",
    "@typescript-eslint/parser": "5.45",
    "babel-jest": "^27.0.2",
    "concurrently": "^6.1.0",
    "cypress": "^13.13.2",
    "cypress-circleci-reporter": "^0.2.0",
    "eslint": "7.32.0",
    "eslint-config-airbnb": "19.0.0",
    "eslint-config-airbnb-typescript": "16.0.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-jest": "25.2.4",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-testing-library": "4.9.0",
    "husky": "^8.0.3",
    "jest": "^27.5.1",
    "lint-staged": "^14.0.1",
    "mockdate": "^3.0.5",
    "@next/eslint-plugin-next": "^14.2.5",
    "prettier": "2.2.1",
    "sass": "^1.32.12",
    "typescript": "^4.2.4"
  }
}
